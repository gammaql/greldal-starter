{
  "name": "greldal-cra-starter-backend",
  "version": "1.0.0",
  "description": "Backend services for greldal-cra-starter",
  "main": "dist/index.js",
  "scripts": {
    "start": "yarn run start:server:dev",
    "start:server:dev": "nodemon --exec babel-node --extensions \".ts\" src/index.ts",
    "start:server:prod": "cross-env NODE_ENV=production node dist/index.js",
    "transpile": "babel ./src --out-dir build --extensions \".ts\"",
    "type-check": "tsc -p .",
    "build": "run-p transpile type-check",
    "test": "jest",
    "migrate:make": "knex migrate:make -x ts",
    "migrate:latest": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "seed:make": "knex seed:make -x ts",
    "seed:run": "knex seed:run"
  },
  "author": "Lorefnon <lorefnon@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "core-js": "2",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "graphql": "^14.2.0",
    "greldal": "^0.10.0",
    "knex": "^0.16.3",
    "pg": "^7.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-typescript": "^7.3.3",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.1",
    "@types/express-graphql": "^0.6.2",
    "@types/graphql": "^14.2.0",
    "@types/inquirer": "^6.0.0",
    "@types/js-yaml": "^3.12.1",
    "@types/lodash": "^4.14.123",
    "@types/node": "^11.12.2",
    "@types/rx": "^4.1.1",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "ts-node": "^8.0.3",
    "typescript": "^3.4.1"
  }
}
