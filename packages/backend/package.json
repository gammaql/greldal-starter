{
  "name": "greldal-starter-backend",
  "version": "1.0.0",
  "description": "Backend services for greldal-cra-starter",
  "main": "dist/index.js",
  "scripts": {
    "start": "yarn run start:server:dev",
    "start:server:dev": "nodemon --exec babel-node --extensions \".ts\" --watch src src/index.ts",
    "start:server:prod": "cross-env NODE_ENV=production node dist/index.js",
    "transpile": "babel ./src --out-dir build --extensions \".ts\"",
    "type-check": "tsc -p .",
    "build": "run-p transpile type-check",
    "test": "jest",
    "migrate:make": "knex migrate:make -x ts",
    "migrate:latest": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "seed:make": "knex seed:make -x ts",
    "seed:run": "knex seed:run"
  },
  "author": "Lorefnon <lorefnon@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "core-js": "3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "graphql": "^14.5.8",
    "greldal": "^0.13.0",
    "io-ts": "^2.0.1",
    "fp-ts": "^2.1.2",
    "knex": "^0.20.3",
    "pg": "^7.14.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/node": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@types/express": "^4.17.2",
    "@types/inquirer": "^6.5.0",
    "@types/js-yaml": "^3.12.1",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.14",
    "@types/rx": "^4.1.1",
    "nodemon": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.2"
  }
}
